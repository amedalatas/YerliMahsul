                                    // Eğer kullanıcı onay verirse
                                    if (confirmLock) {
                                        // Ekranı kilitleme işlemini gerçekleştir
                                        document.getElementById("screenOverlay").style.display = "block";
                                    }
                                }

                                function unlockScreen() {
                                    // Ekranı kilidi açma işlemini gerçekleştir
                                    document.getElementById("screenOverlay").style.display = "none";
                                }
                            </script>
                           
                            <div id="screenOverlay" style="display: none; position: fixed; top: 0; left: 0; width: 100%; height: 100%; background-color: rgba(0, 0, 0, 0.5); z-index: 999;"></div>




