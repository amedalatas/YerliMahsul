                                                <a class="nav-link" asp-controller="Admin" asp-action="Statistics">İstatistikler</a>
                                            }
                                        </li>
                                        @if (SignInManager.IsSignedIn(User))
                                        {
                                            var user = await SignInManager.UserManager.GetUserAsync(User);
                                            var roles = await SignInManager.UserManager.GetRolesAsync(user);
                                            foreach (var rol in roles)
                                            {
                                                if (rol == "Admin")
                                                {
                                                    <li class="nav-item">
                                                        <a class="nav-link" asp-area="" asp-controller="Category" asp-action="Index">Kategori</a>
                                                    </li>
                                                }
                                            }
                                        }
